<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Inherit account move and add Analytic account field -->
    <record model="ir.ui.view" id="account_move_analytic_account_form">
        <field name="name">account.move.analytic.account.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <field name="invoice_incoterm_id" position="before">
                <field name="analytic_account_id"/>
            </field>
        </field>
    </record>
</odoo>
