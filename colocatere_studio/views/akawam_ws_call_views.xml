<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="colocatere_studio_custom_graph_view" model="ir.ui.view">
        <field name="name">akawam.ws.call.custom.graph.view</field>
        <field name="model">akawam.ws.call</field>
        <field name="arch" type="xml">
            <graph string="Akawam Web Service Call">
                <field name="name" required="True" readonly="True"/>
            </graph>
        </field>
    </record>
    <record id="akawam_web_service.akawam_ws_call_action" model="ir.actions.act_window">
        <field name="name">Akawam WS Call</field>
        <field name="res_model">akawam.ws.call</field>
        <field name="view_mode">tree,form,graph</field>
    </record>
</odoo>
