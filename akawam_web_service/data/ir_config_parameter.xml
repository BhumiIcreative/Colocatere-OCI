<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Configuration parameter for Akawam base URL -->
        <record model="ir.config_parameter" id="akawam_base_url">
            <field name="key">akawam.base_url</field>
            <field name="value">URL_BASE</field>
        </record>
        <!-- Configuration parameter for Akawam token -->
        <record model="ir.config_parameter" id="token">
            <field name="key">akawam.token</field>
            <field name="value">TOKEN</field>
        </record>
    </data>
</odoo>
